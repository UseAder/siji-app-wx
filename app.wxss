/**app.wxss**/
@import "weui.wxss";
page{
  background:#f8f8f8;color:#000;
}
.inline-block{display: inline-block;}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 0;
  box-sizing: border-box;
}  
.jinxingz,.yiqianshou{color:#10a651;}
#yushuzhong{background: #85d2a6}

.J-jiedan{
color: #ff5e30!important;
}
.J-jinxingzhong{
  color:  #85d2a6!important;
}
.J-yiwancheng{
  color: #10a651!important;
}

.weui-footer__link {color: #666}
.bd-body-button-yun{position: absolute;bottom: 36rpx;right: 30rpx;border-radius:6rpx;   background: #f8f8f8;      font-size:16px;color:#ffffff;width:216rpx;height:80rpx;}

progress{text-align: center;border-radius:3px;width:100%}

.page__footer-progress{position: relative;display: flex;justify-content: center;color: #fff;border-radius:3px;overflow: hidden;}
.page__footer-progress .progress-text{position: absolute;bottom: 18rpx}


/**弹框**/
.modal-dialog {
  z-index: 2;
      width: 100%;
    height: 100%;
    overflow: hidden;
    position: fixed;
    top: 0;
    left: 0;
   
    background-color:rgba(0,0,0,0.8);
text-align: center;
   
}
.modal-body{
   width: 540rpx;
   
     border-radius: 6rpx;
     background: #fff;
    
      margin:52% auto
}
.modal-title {
  color: #1c2438;
   font-size: 18px;
   line-height: 100rpx;
   padding-top: 15rpx;
}

.modal-content{
  white-space:initial;
  line-height:42rpx;

    font-size: 28rpx;
  color: #80848f;
   
}

.modal-footer {
  margin-top: 30rpx;
    display: flex;
    flex-direction: row;
    height: 86rpx;
  
    border-top: 1px solid rgba(0,0,0,0.1);
    font-size: 34rpx;
    line-height: 86rpx;
}
.btn-confirm {
  
    width: 50%;
    
   line-height:89rpx;
  
    color: #ffffff;
    font-size: 30rpx;
    color: #ed3f14;
    
}
.btn-cancel {
  box-sizing: border-box;
  border-right: 1px solid #e9eaec;
    width: 50%;
    line-height:89rpx;

    font-size: 30rpx;

    color: #495060;
    
}
.btn-confirm:hover, .btn-cancel:hover {
  background: #e9eaec;
}


cover-view{word-break: break-all ; word-wrap: break-word ; white-space: pre-line ;white-space:wrap!important;
}